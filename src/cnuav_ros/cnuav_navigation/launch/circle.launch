<?xml version="1.0"?>
<launch>

<arg name="quad_name" default="hummingbird"/>
    <node pkg="cnuav_navigation" type="circle_trajectory_node" name="traj_node" output="screen">
        <remap from="traj" to="/$(arg quad_name)/trajectory"/>
        
        <remap from="traj" to="/$(arg quad_name)/autopilot/trajectory" />
        <!-- <rosparam file="$(find cnuav_control)/params/autopilot.yaml" /> -->
        <param name="mode" value="true" />
        <remap from="pose" to="/$(arg quad_name)/ground_truth/odometry" />
    </node>
</launch>
