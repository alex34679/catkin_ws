<?xml version="1.0"?>
<launch>

<arg name="quad_name" default="multi3"/>
    <node pkg="cnuav_navigation" type="eight_traj_gen_node" name="traj_node" output="screen">
        <remap from="pose" to="/vrpn_client_node/$(arg quad_name)/pose" />
        <remap from="traj" to="/$(arg quad_name)/trajectory"/>
        <param name="mode" value="false" />
    </node>
</launch>
